{
  "name": "@algorandfoundation/xgov-core",
  "private": true,
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/xgov-core.umd.js",
  "module": "./dist/xgov-core.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/xgov-core.es.js",
      "require": "./dist/xgov-core.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "tsc && vite build",
    "lint": "eslint --ext .ts,.tsx \"src/\"",
    "lint:fix": "eslint --ext .ts,.tsx \"src/\" --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "audit:resolve": "resolve-audit",
    "audit": "check-audit"
  },
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^2.1.0",
    "@noble/ed25519": "^2.0.0",
    "papaparse": "^5.4.1",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "algosdk": "^2.1.0"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.7",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.33.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "npm-audit-resolver": "^3.0.0-RC.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.2"
  }
}
